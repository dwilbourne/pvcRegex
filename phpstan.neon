parameters:
	level: 8
	paths:
		- src
		- tests
	ignoreErrors:

	    -
	        message: '#Call to an undefined method Mockery.*#'
	        path: tests/*
	    -
	        message: '#Mockery\\LegacyMockInterface given\.$#'
	        path: tests/*
	    -
	        message: '#PHPUnit\\Framework\\MockObject\\MockObject given\.$#'
	        path: tests/*

